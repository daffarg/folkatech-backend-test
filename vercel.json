{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/",
        "dest": "/server.js"
      },
      {
        "src": "/auth/(.*)",
        "dest": "/routes/auth.route.js"
      },
      {
        "src": "/api/users/(.*)",
        "dest": "/routes/user.route.js"
      }
    ]
}